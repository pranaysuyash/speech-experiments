{
  "dashboard": {
    "id": null,
    "title": "Model Lab API Dashboard",
    "tags": ["speech", "api", "model-lab"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "5s",
    "panels": [
      {
        "id": 1,
        "title": "API Requests",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "rate(http_requests_total[5m])",
            "legendFormat": "{{method}} {{endpoint}} {{status}}"
          }
        ],
        "yAxes": [
          {
            "label": "Requests/sec"
          }
        ]
      },
      {
        "id": 2,
        "title": "Request Duration",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P95 Duration"
          },
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P50 Duration"
          }
        ],
        "yAxes": [
          {
            "label": "Duration (seconds)"
          }
        ]
      },
      {
        "id": 3,
        "title": "Models in Cache",
        "type": "singlestat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "models_loaded",
            "refId": "A"
          }
        ],
        "valueName": "current",
        "prefix": "",
        "postfix": " models"
      },
      {
        "id": 4,
        "title": "Cache Memory Usage",
        "type": "singlestat",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 8},
        "targets": [
          {
            "expr": "cache_memory_bytes",
            "refId": "A"
          }
        ],
        "valueName": "current",
        "prefix": "",
        "postfix": " bytes"
      },
      {
        "id": 5,
        "title": "ASR vs TTS Requests",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "increase(asr_requests_total[5m])",
            "legendFormat": "ASR Requests"
          },
          {
            "expr": "increase(tts_requests_total[5m])",
            "legendFormat": "TTS Requests"
          }
        ],
        "yAxes": [
          {
            "label": "Requests"
          }
        ]
      },
      {
        "id": 6,
        "title": "Error Rates",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
        "targets": [
          {
            "expr": "rate(errors_total[5m])",
            "legendFormat": "{{type}} - {{endpoint}}"
          }
        ],
        "yAxes": [
          {
            "label": "Errors/sec"
          }
        ]
      }
    ],
    "time": {
      "from": "now-15m",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    }
  }
}